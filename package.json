{
  "name": "main_site",
  "homepage": "https://finside.org",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "gh-pages": "^2.0.0",
    "node-sass": "^6.0.1",
    "query-string": "^6.2.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.3",
    "reactstrap": "^6.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/registerServiceWorker.js",
      "!**/setupTests.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --single-quote --no-semi --write 'src/**/*.js'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "husky": "^1.1.3",
    "jest-fetch-mock": "^1.6.6",
    "js-yaml": "^3.13.1",
    "lint-staged": "^8.0.4",
    "prettier": "1.15.1",
    "prop-types": "^15.6.2"
  }
}

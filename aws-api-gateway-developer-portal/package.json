{
  "name": "aws-serverless-dev-portal-example",
  "version": "2.0.0",
  "description": "Serverless Developer Portal for API Gateway",
  "main": "lambda.js",
  "scripts": {
    "package": "sam package --template-file ./cloudformation/template.yaml --output-template-file ./cloudformation/packaged.yaml --s3-bucket main-site-lambda-backend-functions",
    "install": "npm --prefix ./lambdas/backend install && npm --prefix ./lambdas/listener install",
    "deploy": "sam deploy --template-file ./cloudformation/packaged.yaml --stack-name \"dev-portal\" --capabilities CAPABILITY_NAMED_IAM     MarketplaceSubscriptionTopicProductCode=\"4x7mlfxu4exo7tlo1zyeofno2\" "
  },
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {}
}
